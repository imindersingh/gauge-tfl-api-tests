{
  "type": "object",
  "properties": {
    "startDateTime": {
      "type": "string",
      "format": "date-time"
    },
    "duration": {
      "type": "integer",
      "format": "int32"
    },
    "arrivalDateTime": {
      "type": "string",
      "format": "date-time"
    },
    "legs": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "duration": {
            "type": "integer",
            "format": "int32"
          },
          "speed": {
            "type": "string"
          },
          "instruction": {
            "type": "object",
            "properties": {
              "summary": {
                "type": "string"
              },
              "detailed": {
                "type": "string"
              },
              "steps": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "description": {
                      "type": "string"
                    },
                    "turnDirection": {
                      "type": "string"
                    },
                    "streetName": {
                      "type": "string"
                    },
                    "distance": {
                      "type": "integer",
                      "format": "int32"
                    },
                    "cumulativeDistance": {
                      "type": "integer",
                      "format": "int32"
                    },
                    "skyDirection": {
                      "type": "integer",
                      "format": "int32"
                    },
                    "skyDirectionDescription": {
                      "enum": [
                        "North",
                        "NorthEast",
                        "East",
                        "SouthEast",
                        "South",
                        "SouthWest",
                        "West",
                        "NorthWest"
                      ],
                      "type": "string"
                    },
                    "cumulativeTravelTime": {
                      "type": "integer",
                      "format": "int32"
                    },
                    "latitude": {
                      "type": "number",
                      "format": "double"
                    },
                    "longitude": {
                      "type": "number",
                      "format": "double"
                    },
                    "pathAttribute": {
                      "type": "object",
                      "properties": {
                        "name": {
                          "type": "string"
                        },
                        "value": {
                          "type": "string"
                        }
                      }
                    },
                    "descriptionHeading": {
                      "type": "string"
                    },
                    "trackType": {
                      "enum": [
                        "CycleSuperHighway",
                        "CanalTowpath",
                        "QuietRoad",
                        "ProvisionForCyclists",
                        "BusyRoads",
                        "None",
                        "PushBike",
                        "Quietway"
                      ],
                      "type": "string"
                    }
                  }
                }
              }
            }
          },
          "obstacles": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "type": {
                  "type": "string"
                },
                "incline": {
                  "type": "string"
                },
                "stopId": {
                  "type": "integer",
                  "format": "int32"
                },
                "position": {
                  "type": "string"
                }
              }
            }
          },
          "departureTime": {
            "type": "string",
            "format": "date-time"
          },
          "arrivalTime": {
            "type": "string",
            "format": "date-time"
          },
          "departurePoint": {
            "type": "object",
            "properties": {
              "lat": {
                "type": "number",
                "description": "WGS84 latitude of the location.",
                "format": "double"
              },
              "lon": {
                "type": "number",
                "description": "WGS84 longitude of the location.",
                "format": "double"
              }
            },
            "description": "Represents a point located at a latitude and longitude using the WGS84 co-ordinate system."
          },
          "arrivalPoint": {
            "type": "object",
            "properties": {
              "lat": {
                "type": "number",
                "description": "WGS84 latitude of the location.",
                "format": "double"
              },
              "lon": {
                "type": "number",
                "description": "WGS84 longitude of the location.",
                "format": "double"
              }
            },
            "description": "Represents a point located at a latitude and longitude using the WGS84 co-ordinate system."
          },
          "path": {
            "type": "object",
            "properties": {
              "lineString": {
                "type": "string"
              },
              "stopPoints": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "string"
                    },
                    "name": {
                      "type": "string"
                    },
                    "uri": {
                      "type": "string"
                    },
                    "fullName": {
                      "type": "string"
                    },
                    "type": {
                      "type": "string"
                    },
                    "crowding": {
                      "type": "object",
                      "properties": {
                        "passengerFlows": {
                          "type": "array",
                          "items": {
                            "type": "object",
                            "properties": {
                              "timeSlice": {
                                "type": "string",
                                "description": "Time in 24hr format with 15 minute intervals e.g. 0500-0515, 0515-0530 etc."
                              },
                              "value": {
                                "type": "integer",
                                "description": "Count of passenger flow towards a platform",
                                "format": "int32"
                              }
                            }
                          },
                          "description": "Busiest times at a station (static information)"
                        },
                        "trainLoadings": {
                          "type": "array",
                          "items": {
                            "type": "object",
                            "properties": {
                              "line": {
                                "type": "string",
                                "description": "The Line Name e.g. \"Victoria\""
                              },
                              "lineDirection": {
                                "type": "string",
                                "description": "Direction of the Line e.g. NB, SB, WB etc."
                              },
                              "platformDirection": {
                                "type": "string",
                                "description": "Direction displayed on the platform e.g. NB, SB, WB etc."
                              },
                              "direction": {
                                "type": "string",
                                "description": "Direction in regards to Journey Planner i.e. inbound or outbound"
                              },
                              "naptanTo": {
                                "type": "string",
                                "description": "Naptan of the adjacent station"
                              },
                              "timeSlice": {
                                "type": "string",
                                "description": "Time in 24hr format with 15 minute intervals e.g. 0500-0515, 0515-0530 etc."
                              },
                              "value": {
                                "type": "integer",
                                "description": "Scale between 1-6, \r\n             1 = Very quiet, 2 = Quiet, 3 = Fairly busy, 4 = Busy, 5 = Very busy, 6 = Exceptionally busy",
                                "format": "int32"
                              }
                            }
                          },
                          "description": "Train Loading on a scale 1-6, 1 being \"Very quiet\" and 6 being \"Exceptionally busy\" (static information)"
                        }
                      }
                    },
                    "routeType": {
                      "enum": [
                        "Unknown",
                        "All",
                        "Cycle Superhighways",
                        "Quietways",
                        "Cycleways",
                        "Mini-Hollands",
                        "Central London Grid",
                        "Streetspace Route"
                      ],
                      "type": "string"
                    },
                    "status": {
                      "enum": [
                        "Unknown",
                        "All",
                        "Open",
                        "In Progress",
                        "Planned",
                        "Planned - Subject to feasibility and consultation.",
                        "Not Open"
                      ],
                      "type": "string"
                    }
                  }
                }
              },
              "elevation": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "distance": {
                      "type": "integer",
                      "format": "int32"
                    },
                    "startLat": {
                      "type": "number",
                      "format": "double"
                    },
                    "startLon": {
                      "type": "number",
                      "format": "double"
                    },
                    "endLat": {
                      "type": "number",
                      "format": "double"
                    },
                    "endLon": {
                      "type": "number",
                      "format": "double"
                    },
                    "heightFromPreviousPoint": {
                      "type": "integer",
                      "format": "int32"
                    },
                    "gradient": {
                      "type": "number",
                      "format": "double"
                    }
                  }
                }
              }
            }
          },
            "$ref": "#/components/schemas/Tfl-12"
          },
          "routeOptions": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Tfl-13"
            }
          },
          "mode": {
            "$ref": "#/components/schemas/Tfl-10"
          },
          "disruptions": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Tfl-21"
            }
          },
          "plannedWorks": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Tfl-22"
            }
          },
          "distance": {
            "type": "number",
            "format": "double"
          },
          "isDisrupted": {
            "type": "boolean",
            "readOnly": true
          },
          "hasFixedLocations": {
            "type": "boolean",
            "readOnly": true
          }
        }
      }
    },
    "fare": {
      "$ref": "#/components/schemas/Tfl-28"
    }
  },
  "description": "Object that represents an end to end journey (see schematic)."
}